[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = Kyle Crawshaw
	email = kyle.crawshaw@gmail.com
[push]
	default = simple
[credential]
	helper = osxkeychain
[core]
	excludesfile = ~/.gitignore_global
	editor = nvim
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	d = difftool
